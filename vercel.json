{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/execute",
      "dest": "api/index.py",
      "headers": {
        "X-Auth-Skip": "true"
      },
      "continue": true
    },
    {
      "src": "/api/feed",
      "dest": "api/index.py",
      "headers": {
        "X-Auth-Skip": "true"
      },
      "continue": true
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
